{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.map": {
            "when": "$(basename)"
        },
        "**/*.js": {
            "when": "$(basename).ts"
        },
        "**/yame.js": true
    }
}
